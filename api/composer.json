{
    "name": "stmarkreformed/stmarkreformed",
    "version": "dev-stmarkreformed",
    "license": "proprietary",
    "require": {
        "php": "^8.4",
        "ext-pdo": "*",
        "ext-redis": "*",
        "buzzingpixel/php-queue": "^2.0",
        "buzzingpixel/php-scheduler": "^1.1",
        "hyperf/guzzle": "^3.1",
        "johngrogg/ics-parser": "^3.4",
        "lcobucci/clock": "^3.3",
        "monolog/monolog": "^3.9",
        "rxante/dateimmutable": "^1.0",
        "rxante/php-app-bootstrap": "^1.0.1",
        "spatie/php-cloneable": "^1.0",
        "symfony/cache": "^7.2"
    },
    "require-dev": {
        "buzzingpixel/symfony-var-dumper-decorator": "^1.0",
        "filp/whoops": "^2.15",
        "rxante/coding-standards": "^1.1.2",
        "symfony/var-dumper": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Config\\": "config",
            "App\\": "src",
            "Public\\": "public",
            "Storage\\": "storage"
        }
    },
    "config": {
        "bin-compat": "full",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "pestphp/pest-plugin": true
        }
    }
}
