{
    "name": "st-mark-reformed/stmarkreformed.com",
    "description": "St. Mark Reformed Church Website",
    "license": "proprietary",
    "homepage": "http://stmarkreformed.com/",
    "authors": [
        {
            "name": "TJ Draper",
            "email": "tj@buzzingpixel.com",
            "homepage": "https://buzzingpixel.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0.13",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-redis": "*",
        "ext-simplexml": "*",
        "buzzingpixel/bp-cache": "^1.0",
        "buzzingpixel/craft-scheduler": "^1.0",
        "buzzingpixel/craft-slim-bridge": "^1.1.5",
        "buzzingpixel/minify-middleware": "^1.0",
        "buzzingpixel/static-cache-middleware": "^1.0",
        "buzzingpixel/twig-markdown": "^1.0",
        "buzzingpixel/twig-smartypants": "^1.0",
        "buzzingpixel/twig-widont": "^1.0",
        "cocur/slugify": "^4.0.0",
        "craftcms/ckeditor": "1.1.2",
        "craftcms/cms": "3.7.24",
        "craftcms/redactor": "2.8.8",
        "daverandom/resume": "^0.0.3",
        "doctrine/annotations": "^1.13",
        "dolphiq/redirect": "1.1.1",
        "doublesecretagency/craft-inventory": "2.1.1",
        "elasticsearch/elasticsearch": "^7.15",
        "enupal/stripe": "3.3.6",
        "gumlet/php-image-resize": "^2.0",
        "lewisjenkins/craft-range-slider": "^1.0",
        "lucidtaz/yii2-whoops": "dev-master",
        "michelf/php-smartypants": "1.8.1",
        "percipioglobal/craft-colour-swatches": "^1.4",
        "php-di/php-di": "^6.3",
        "php-imap/php-imap": "^2.0",
        "sebastianlenz/linkfield": "^1.0",
        "slim/flash": "^0.4.0",
        "solspace/craft3-calendar": "^3.3",
        "spatie/image-optimizer": "^1.5",
        "supercool/buttonbox": "^2.0",
        "tde/craftplugin-geoaddress": "1.0.13",
        "timostamm/text-truncation": "dev-master",
        "vaersaagod/matrixmate": "^1.2",
        "verbb/super-table": "^2.6",
        "wrav/oembed": "^1",
        "yiisoft/yii2-redis": "^2.0"
    },
    "require-dev": {
        "buzzingpixel/twig-dumper": "^1.0",
        "codedungeon/phpunit-result-printer": "^0.31.0",
        "doctrine/coding-standard": "^9.0",
        "friendsofphp/php-cs-fixer": "^2.18",
        "morrislaptop/var-dumper-with-context": "^0.3.2",
        "phpstan/phpstan": "^0.12.99",
        "phpstan/phpstan-deprecation-rules": "^0.12.6",
        "phpstan/phpstan-strict-rules": "^0.12.11",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/var-dumper": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src",
            "Config\\": "config"
        }
    },
    "config": {
        "bin-compat": "full",
        "sort-packages": true,
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
