{
    "name": "st-mark-reformed/stmarkreformed.com",
    "description": "St. Mark Reformed Church Website",
    "license": "proprietary",
    "homepage": "http://stmarkreformed.com/",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "craftcms/cms": "^3.0.0",
        "vlucas/phpdotenv": "^2.4.0",
        "nystudio107/craft-minify": "^1.2",
        "buzzingpixel/craft-static": "^1.0.2",
        "michelf/php-smartypants": "^1.8",
        "superbig/craft-entry-instructions": "^1.0",
        "tde/craftplugin-geoaddress": "^1.0",
        "craftcms/ckeditor": "dev-master",
        "cocur/slugify": "^3.1",
        "timostamm/text-truncation": "dev-master",
        "lucidtaz/yii2-whoops": "dev-master",
        "digitalnature/php-ref": "^1.2",
        "aelvan/imager": "^2.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.4",
        "phpmd/phpmd": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "dev\\": "dev/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.1"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:craftcms/ckeditor.git"
        }
    ]
}
