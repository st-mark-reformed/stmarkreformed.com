{
    "name": "st-mark-reformed/stmarkreformed.com",
    "description": "St. Mark Reformed Church Website",
    "license": "proprietary",
    "homepage": "http://stmarkreformed.com/",
    "authors": [
        {
            "name": "TJ Draper",
            "email": "tj@buzzingpixel.com",
            "homepage": "https://buzzingpixel.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:craftcms/ckeditor.git"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "ext-dom": "*",
        "ext-json": "*",
        "aelvan/imager": "2.4.0",
        "buzzingpixel/craft-static": "2.0.1",
        "cocur/slugify": "3.2",
        "craftcms/ckeditor": "dev-master",
        "craftcms/cms": "^3.0.0",
        "craftcms/redactor": "2.3.3.2",
        "digitalnature/php-ref": "1.3",
        "doublesecretagency/craft-inventory": "2.0.3",
        "lucidtaz/yii2-whoops": "dev-master",
        "michelf/php-smartypants": "1.8.1",
        "nystudio107/craft-minify": "1.2.10",
        "superbig/craft-entry-instructions": "1.0.7",
        "tde/craftplugin-geoaddress": "1.0.12",
        "timostamm/text-truncation": "dev-master"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "src\\": "src/"
        }
    },
    "config": {
        "bin-compat": "full",
        "sort-packages": true
    }
}
